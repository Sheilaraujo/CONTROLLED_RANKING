 <div id="">
  <h1>Parceiros para meus jogos</h1>
  <%  "Meu user_id = #{user.user_id} - #{user.name}" %>

    <ul>
      <% @schedules.each do |schedule|%>
        <% if (schedule.user_id == user.user_id) then %>
          <li><%= "#{schedule.date} - #{schedule.place} - Parceiros: 1-#{schedule.partner[0]} 2-#{schedule.partner[0]} 3-#{schedule.partner[0]}" %></li>
        <% end %>
      <% end %>
    </ul>
  </div>
